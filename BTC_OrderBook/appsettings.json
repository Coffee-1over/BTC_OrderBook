{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost\\SQLEXPRESS;Database=OrderBook;Trusted_Connection=True;TrustServerCertificate=True;"
  },
  "Bitstamp": {
    "ApiUrl": "https://www.bitstamp.net",
    "OrderBook": {
      "Link": "/api/v2/order_book/{0}"
    }
  },
  "Hub": {
    "OrderBookHub": {
      "DelayInSeconds": 30
    }
  },
  "AllowedHosts": "*"
}
